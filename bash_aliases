# Just some of my aliases I have in the bashrc file that I find useful

alias grep_cidr="grep -o '[0-9]\{1,3\}\.[0-9]\{1,3\}\.[0-9]\{1,3\}\.[0-9]\{1,3\}\/[0-9]\{1,\}'"
alias grep_ip="grep -E -o '(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)'"
alias mkdir="mkdir -p"
alias shred="shred -uzf"
alias burp="/usr/local/bin/BurpSuiteCommunity"
alias john="/home/alex/Tools/john/run/john"
